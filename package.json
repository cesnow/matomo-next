{
  "name": "@cesnow/matomo-next",
  "version": "1.0.1",
  "description": "Matomo integration for next.js projects",
  "source": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "license": "MPL-2.0",
  "author": "EvanLater",
  "homepage": "https://github.com/cesnow/matomo-next#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesnow/matomo-next.git",
    "directory": "src"
  },
  "bugs": {
    "url": "https://github.com/cesnow/matomo-next/issues"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "contributors": [
    "Chris van Mook <chrisvanmook@gmail.com>",
    "Jon Koops <jonkoops@gmail.com>",
    "Max Fahl <max@fahl.se>",
    "Evan Later <cesnow.lab@gmail.com>"
  ],
  "keywords": [
    "matomo",
    "tracking",
    "analytics",
    "nextjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "build:esm": "tsc -b tsconfig.esm.json",
    "lint": "eslint .",
    "test": "jest",
    "clean": "rm -rf dist",
    "prettier": "prettier --write ./src/**/**/*.{ts,tsx}",
    "clean:generated": "find . -name \"*.d.ts\" -type f -delete && find . -name \"*.tsbuildinfo\" -type f -delete",
    "start": "yarn build:cjs -- --watch & yarn build:esm -- --watch",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}
